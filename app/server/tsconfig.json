{
	"compilerOptions": {
		"lib": [
			"dom",
			"ES2020",
			"ES2020.string"
		],
		"downlevelIteration": true,
		"esModuleInterop": true,
		"module": "commonjs",
		"target": "ES2015",
		"noEmit": true,
		"paths": {
			"@shared/*": [
				"../shared/*"
			]
		},
		"baseUrl": "./"
	},
	"exclude": ["thumbs", "logs", "processing"],
	"include": ["src"],
}