{
	"compilerOptions": {
		"lib": [
			"dom",
			"ES2021"
		],
		"downlevelIteration": true,
		"esModuleInterop": true,
		"module": "commonjs",
		"target": "ES2015",
		"noEmit": true,
		"paths": {
			"@shared/*": [
				"../shared/*"
			]
		},
	},
	"watchOptions": {
		"excludeDirectories": ["thumbs", "logs", "processing"],
	}
}